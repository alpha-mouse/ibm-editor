{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-MM-ddTHH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=files/Editor.db"
  },
  "Settings": {
    "GrammarDbPath": "files/grammar.db"
  },
  "AwsSettings": {
    "AccessKeyId": "",
    "SecretAccessKey": "",
    "Region": "eu-central-1",
    "BucketName": "ibm-editor"
  },
  "AllowedHosts": "*"
}
